{
  "realm": "scouting-outing",
  "enabled": true,
  "displayName": "Scouting Outing Manager",
  "registrationAllowed": true,
  "registrationEmailAsUsername": true,
  "rememberMe": true,
  "verifyEmail": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "resetPasswordAllowed": true,
  "editUsernameAllowed": false,
  "bruteForceProtected": true,
  "permanentLockout": false,
  "maxFailureWaitSeconds": 900,
  "minimumQuickLoginWaitSeconds": 60,
  "waitIncrementSeconds": 60,
  "quickLoginCheckMilliSeconds": 1000,
  "maxDeltaTimeSeconds": 43200,
  "failureFactor": 30,
  "clients": [
    {
      "clientId": "scouting-outing-backend",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "dev-client-secret-change-in-production",
      "redirectUris": [
        "http://localhost:8000/*",
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "http://localhost:8000",
        "http://localhost:3000"
      ],
      "protocol": "openid-connect",
      "publicClient": false,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": true,
      "authorizationServicesEnabled": false
    },
    {
      "clientId": "scouting-outing-frontend",
      "enabled": true,
      "publicClient": true,
      "redirectUris": [
        "http://localhost:3000/*",
        "http://localhost:3000/auth/callback"
      ],
      "webOrigins": [
        "http://localhost:3000",
        "http://localhost:8000"
      ],
      "protocol": "openid-connect",
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "parent",
        "description": "Parent/Guardian role for managing family accounts"
      },
      {
        "name": "admin",
        "description": "Administrator role"
      }
    ]
  },
  "users": [],
  "defaultRoles": [
    "offline_access",
    "uma_authorization"
  ]
}

