# Production Environment Variables
# Copy this file to .env and update with your actual values

# Database Configuration
POSTGRES_USER=som_owner
POSTGRES_PASSWORD=CHANGE_THIS_TO_A_SECURE_PASSWORD
POSTGRES_DB=scouting_outing_manager

# Keycloak Database Configuration
KEYCLOAK_DB_USER=keycloak
KEYCLOAK_DB_PASSWORD=CHANGE_THIS_TO_A_SECURE_KEYCLOAK_DB_PASSWORD

# Backend Configuration
SECRET_KEY=CHANGE_THIS_TO_A_SECURE_RANDOM_STRING
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Initial Admin User
INITIAL_ADMIN_EMAIL=admin@example.com
INITIAL_ADMIN_PASSWORD=CHANGE_THIS_TO_A_SECURE_ADMIN_PASSWORD

# Keycloak Configuration
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=CHANGE_THIS_TO_A_SECURE_KEYCLOAK_ADMIN_PASSWORD
KEYCLOAK_REALM=scouting-outing
KEYCLOAK_CLIENT_ID=scouting-outing-backend
KEYCLOAK_CLIENT_SECRET=CHANGE_THIS_TO_A_SECURE_CLIENT_SECRET
FRONTEND_URL=http://your-domain.com

# CORS Configuration (update with your domain)
BACKEND_CORS_ORIGINS=http://your-domain.com,https://your-domain.com

# Frontend Configuration
VITE_API_URL=/api