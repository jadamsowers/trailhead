# Docker Compose Environment Variables Example
# Copy this file to .env and update with your values

# Frontend Environment Variables
VITE_API_URL=http://localhost:8001/api

# Database Configuration
POSTGRES_USER=scouting_outing
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=scouting_outing_manager

# Security
SECRET_KEY=your_secret_key_here
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_DAYS=7

# CORS
BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:8000,http://localhost:8080

# Initial Admin User
INITIAL_ADMIN_EMAIL=admin@example.com
INITIAL_ADMIN_PASSWORD=your_admin_password_here

# Keycloak Configuration
KEYCLOAK_ADMIN_USER=admin
KEYCLOAK_ADMIN_PASSWORD=your_keycloak_admin_password_here
KEYCLOAK_DB_USER=keycloak
KEYCLOAK_DB_PASSWORD=your_keycloak_db_password_here
KEYCLOAK_REALM=scouting-outing
KEYCLOAK_CLIENT_SECRET=your_client_secret_here
FRONTEND_URL=http://localhost:3000